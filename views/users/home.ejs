    </div>

    <!--  End Drop of the day -->

    <!-- Start Product Section -->
    <div class="product-section">
      <div class="container">
        <div class="row">
          <!-- Start Column 1 -->
          <h3 class="populcoll" style="margin-bottom: 50px">
            <span style="color: #3b5d50">POPULAR</span> CATEGORIES
          </h3>
          <br />
          <% category.forEach((data) => {%>

            <div class="col-12 col-md-4 col-lg-3 mb-5 mb-md-0">
              <a class="product-item" href="cart.html">
                <img
                  src="<%= data.image %>"
                  class="img-fluid product-thumbnail"
                />
  
                <h3 class="product-title"><%= data.name %></h3>
  
                <span class="icon-cross">
                  <img src="/images/cross.svg" class="img-fluid" />
                </span>
              </a>
            </div>
   
          <%})%>

         
        </div>
      </div>
    </div>

    <!-- start New arrivals -->
    <!-- start New arrivals -->
    <h2 class="ms-3 newarrivalnew">
      <span style="color: #3b5d50">NEW </span>ARRRIVALS
    </h2>
    <div class="container-fluid">
      <div class="row justify-content-center">
        <% products.forEach((data, index) => { %>
          <div class="col-12 col-md-4 col-lg-3 mb-md-0 newarrnew text-center">
            <div class="newarrimg-box">
              <img class="newarrimg" src="<%= data.images[0] %>" alt="" />
              <i class="fa-regular fa-heart fa-xl mt-4 wishiconnew" style="color: #000000"></i>
            </div>
            <div class="newarrcolbox1new">
              <a class="product-item" href="cart.html">
                <div class="tittlesnew">
                  <% if (index === 0) { %> SHIRTS <% } %>
                  <% if (index === 1) { %> PANTS <% } %>
                  <% if (index === 2) { %> OVERSIZED T-SHIRTS <% } %>
                  <% if (index === 3) { %> FULL SLEEVE <% } %>
                </div>
              </a>
            </div>
          </div>
        <% }) %>
      </div>
