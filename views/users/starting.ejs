  </head>

  <body>
  
    <nav class="custom-navbar navbar navbar navbar-expand-md" arial-label="Furni navigation bar" >
      <div class="container">
        <div id="navlogo" >
          <a href="/home"><img src="/images/img/logo/A_logo_with_the_letters_M_and_H_together_with_a_cap_on_M-removebg-preview.png" alt=""></a>
        </div>
        <a class="navbar-brand" href="/home"><span class="brandname">Men's Hub </span></a>
    
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarsFurni"
          aria-controls="navbarsFurni"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
    
        <div class="collapse navbar-collapse" id="navbarsFurni">
          <ul class="custom-navbar-nav navbar-nav ms-auto mb-2 mb-md-0">
            <li class="nav-item active">
              <a class="nav-link" href="/home">Home</a>
            </li>
            <li><a class="nav-link" href="/login">Shop</a></li>
            <li><a class="nav-link" href="/about">About us</a></li>
            <li><a class="nav-link" href="/contact">Contact us</a></li>
          </ul>
          <ul class="neddit">
            <li class="neddit1">
              <a style="text-decoration: none;" href="/login"><p style="margin-left: 11px; margin-bottom:0; color:rgb(185, 182, 182); text-decoration: none;">login/Register</p></a>
              
            </li>
          </ul>
    
          <ul class="custom-navbar-cta navbar-nav mb-2 mb-md-0 ms-5">
            <li>
              <a href="wishlist.html">
                <a href="/login"><i class="fa fa-heart wishicon2" style="color: white;"> </i></a>        
              </a>
            </li>
            <li>
              <a class="nav-link" href="/login"><img src="/images/user.svg" /></a>
            </li>
            <li>
              <a class="nav-link" href="#"
                ><img src="/images/cart.svg"
              /></a>
            </li>
          </ul>
        </div>
      </div>
    </nav>


    <div class="container-fluid homebanner"></div>
    

    <!-- Start Product Section -->
    <div class="product-section">
      <div class="container">
        <div class="row">
          <h3 class="populcoll" style="margin-bottom: 50px"><span style="color: #3b5d50">POPULAR</span> CATEGORIES</h3>
          <% category.forEach((data) => { %>
            <div class="col-12 col-md-4 col-lg-3 mb-5 mb-md-0">
              <a class="product-item" href="/login">
                <img src="<%= data.image %>" class="img-fluid product-thumbnail" />
                <h3 class="product-title"><%= data.name %></h3>
                <span class="icon-cross"><img src="/images/cross.svg" class="img-fluid" /></span>
              </a>
            </div>
          <% }) %>
        </div>
      </div>
    </div>

    <!-- Start New Arrivals -->
    <h2 class="ms-3 newarrivalnew"><span style="color: #3b5d50">NEW </span>ARRIVALS</h2>
    <div class="container-fluid">
      <div class="row">
        <% products.forEach((data, index) => { %>
          <div class="col-12 col-md-4 col-lg-3 mb-md-0 newarrnew text-center" style="margin-top: 20px;">
            <div class="newarrimg-box">
              <a href="/login">
                <img class="newarrimg" src="/<%= data.images[0] %>" alt="<%= data.name %>" />
              </a>
              <i class="fa-regular fa-heart fa-xl mt-4 wishiconnew" style="color: #000000"></i>
            </div>
            <div class="newarrcolbox1new">
              <a class="product-item" href="/login" style="text-decoration: none;">
                <div class="tittlesnew"><%= data.name %></div>
              </a>
            </div>
          </div>
        <% }) %>
        
      </div>
    </div>

    <br>
    <br>
<%- include ("./layouts/footer.ejs") %>    

    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/tiny-slider.js"></script>
    <script src="js/custom.js"></script>
  </body>
</html>
